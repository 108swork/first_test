.gen_menu
  display none
  @extend .mautota
  word-spacing -0.2rem
  {tac}
  width auto
  li
    display inline-block
    a
      display inline-block
      font(2.5rem,#fff,1)
      mp(0,1.6rem 2.8rem)
      background-color #8ec1e3
      &:hover
        transition 1s
        text-shadow 0 0 4px #fff
    &:nth-child(1) a
      background-color #8ec1e3
      transition 1s
      &:hover
        background-color darken(#8ec1e3,20%)
    &:nth-child(2) a
      background-color #77afd4
      transition: 1s
      &:hover
        background-color darken(#77afd4,20%)
    &:nth-child(3) a
      background-color #62a3ce
      transition 1s
      &:hover
        background-color darken(#62a3ce,20%)
    &:nth-child(4) a
      background-color #4f97c6
      transition 1s
      &:hover
        background-color darken(#4f97c6,20%)
    &:nth-child(5) a
      background-color #1b88bb
      transition 1s
      &:hover
        background-color darken(#1b88bb,20%)
    &:nth-child(6) a
      background-color #1279a9
      transition 1s
      &:hover
        background-color darken(#1279a9,20%)
